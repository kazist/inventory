    "query": {
        "from": {
            "table": "Inventory\\Transit\\Products\\Code\\Tables\\Products",
            "alias": "itp",
            "name": "#__inventory_transit_products"
        },
        "select": [
            "itp.id",
            "itp.product_id",
            "pp.title AS product_id_title",
            "itp.transit_id",
            "it.vehicle_id AS transit_id_vehicle_id",
            "itp.quantity",
            "itp.created_by",
            "itp.date_created",
            "itp.modified_by",
            "itp.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Products\\Products\\Code\\Tables\\Products",
                "alias": "pp",
                "name": "#__products_products",
                "on": "itp.product_id = pp.id"
            },
            {
                "table": "Inventory\\Transit\\Code\\Tables\\Transit",
                "alias": "it",
                "name": "#__inventory_transit",
                "on": "itp.transit_id = it.id"
            }
        ]
    }
